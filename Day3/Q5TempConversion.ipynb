   "source": [
    "# Conversion of fahrenheit to kelvin and celsius "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter temp in fahrenheit -50\n",
      "Fahrenheit= -50 F Celsius= -45.55555555555556 C Kelvin= 227.59444444444446 K\n"
     ]
    }
   ],
   "source": [
    "f=int(input(\"Enter temp in fahrenheit \"))\n",
    "c=(f-32)*(5/9)\n",
    "k=(f+459.67)*(5/9)\n",
    "print(\"Fahrenheit=\",f,\"F Celsius=\",c,\"C Kelvin=\",k,\"K\")"
   ]
  },
